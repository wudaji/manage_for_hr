    width:15px;
    height:3px;
    display: block;
    background: url("../images/11.png") center no-repeat;
    position: absolute;
    top:21px;
    right:12px;    
    color:#fff;  
    background-size: cover; 
}
.pro-manage .add{
    width:16px;
    height:16px;
    display: block;
    background: url("../images/10.png") center no-repeat;
    position: absolute;
    top:15px;
    right:12px;
    color:#fff;  
    background-size: cover; 
    display: none;  
}

  .left-side .pro-contract-first{
    overflow: hidden;
    width:180px;  
    background-color: #35404d;    
    /* padding-left: 25px;    */
