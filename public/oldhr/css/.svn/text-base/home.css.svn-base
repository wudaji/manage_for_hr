.header {
    width: 100%;
    height: 80px;
    position: fixed;
    top: 0;
    left: 0;   
    z-index: 999;
    background: #fff;
   
}

.l-header {
    float: left;
    width: 372px;
    height:35px;   
    background: url("../images/4.png") center;
    margin-left: 22px;
    margin-top: 22px;  
   }

.r-header{    
    height:34px;
    float: right;
    
}

.r-header li {
    float: left;
    width:151px;
    height:34px; 
    margin-right: 10px;
    text-align: center;
    line-height: 34px;
    margin-top: 21px;
    border:1px solid #f1f1f1;
    background-color: #f9f9f9;    

}
.r-header .hr-name {
    width:300px;
}


.r-header .adn {
    width:27px;
    height:27px;
    display:inline-block;  
    margin-right: 18px;
    background: url("../images/5.png") center no-repeat;
    vertical-align: middle;   
}
.r-header .updat{
    width:16px;
    height:16px;
    display: inline-block;
    background:url("../images/6.png") center no-repeat;
    cursor: pointer;
    margin-right: 8px;
    vertical-align: middle;   
}

.r-header a {
    color:#5e5e5e;
}
.r-header .back {
    width:16px;
    height:16px;
    display: inline-block;
    background:url("../images/7.png") center;
    cursor: pointer;
    margin-right: 8px;
    vertical-align: middle;
}
.r-header .u-pwd {
    width:99px;
}
.r-header .b-login {
    width:99px;
}


